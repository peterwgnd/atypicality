header
	z-index set-index('overlay')
	position absolute
	top 0
	left 0
	width 100%

	a.logo
		display block
		border none
		max-width 9rem
		margin 1rem auto

		svg
			fill $off-black
			display block
			width 100%
			
nav
	max-width 1040px
	margin 0 auto
	padding 1rem 0
	position relative
	text-align center
	
	a
		max-width 200px
		display inline-block
		
	a.logo-cbs
		position absolute
		left 0
		
	a.logo-ii
		padding-top 12px